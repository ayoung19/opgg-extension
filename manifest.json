{
    "manifest_version": 3,
    "name": "OP.GG Summoner Search",
    "description": "Search for the games that you have played with a certain summoner.",
    "version": "4.0.0",
    "content_scripts": [
        {
            "matches": ["*://*.op.gg/summoner/userName=*"],
            "js": ["jquery-3.1.0.min.js", "content.js"]
        }
    ],
    "icons": {
        "16": "opgg16.png",
        "48": "opgg48.png",
        "128": "opgg128.png"
    }
}

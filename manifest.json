{
    "manifest_version": 3,
    "name": "OP.GG Summoner Search",
    "description": "Filter for games you've played with a specific summoner on OP.GG.",
    "version": "4.0.0",
    "content_scripts": [
        {
            "matches": ["*://*.op.gg/summoner/userName=*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "opgg16.png",
        "48": "opgg48.png",
        "128": "opgg128.png"
    }
}
